def celsius_to_fahrenheit(celsius):
    return (celsius * 9/5) + 32

def celsius_to_kelvin(celsius):
    return celsius + 273.15

def fahrenheit_to_celsius(fahrenheit):
    return (fahrenheit - 32) * 5/9

def fahrenheit_to_kelvin(fahrenheit):
    return (fahrenheit + 459.67) * 5/9

def kelvin_to_celsius(kelvin):
    return kelvin - 273.15

def kelvin_to_fahrenheit(kelvin):
    return kelvin * 9/5 - 459.67
import temperature_converter as tc
celsius = 25
fahrenheit = tc.celsius_to_fahrenheit(celsius)
print(f"{celsius} degrees Celsius is {fahrenheit:.2f} degrees Fahrenheit")
fahrenheit = 98.6
kelvin = tc.fahrenheit_to_kelvin(fahrenheit)
print(f"{fahrenheit} degrees Fahrenheit is {kelvin:.2f} Kelvin")
kelvin = 300
celsius = tc.kelvin_to_celsius(kelvin)
print(f"{kelvin} Kelvin is {celsius:.2f} degrees Celsius")