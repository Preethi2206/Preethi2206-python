def add(x, y):
    return x + y

def subtract(x, y):
    return x - y

def multiply(x, y):
    return x * y

def divide(x, y):
    if y == 0:
        raise ValueError("Cannot divide by zero!")
    return x / y
import arithmetic_operations as ao
x, y = 10, 5
print(f"{x} + {y} = {ao.add(x, y)}")
print(f"{x} - {y} = {ao.subtract(x, y)}")
print(f"{x} * {y} = {ao.multiply(x, y)}")
print(f"{x} / {y} = {ao.divide(x, y)}")